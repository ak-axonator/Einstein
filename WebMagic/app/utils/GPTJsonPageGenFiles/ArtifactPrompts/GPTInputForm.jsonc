{
    //What is the name of your form?
    "ArtifactName": "",
    "Prompts": [
        //generate form prompt
        {
            "Enabled": true,
            "PrefixInstruction": "Give a Form: ",
            "Artifact": "form",
            "GiveContentFor": [
                {
                    "JSONKey": "form_title",
                    "JSONValue": "form main title"
                },
                {
                    "JSONKey": "form_description",
                    "JSONValue": "form description in 200 words"
                },
                {
                    "JSONKey": "users",
                    "JSONValue": "array of 4 users of the form in this subcategory"
                },
                {
                    "JSONKey": "user_description",
                    "JSONValue": "description of the users involved in the app in this subcategory in 30 words"
                },
                {
                    "JSONKey": "use",
                    "JSONValue": "json of how to use this form with and without Axonator"
                },
                {
                    "JSONKey": "use_with_axonator",
                    "JSONValue": "description in 10 to 15 words on how to use this form with Axonator"
                },
                {
                    "JSONKey": "use_without_axonator",
                    "JSONValue": "description in 10 to 15 words on how to use this form without Axonator"
                },
                {
                    "JSONKey": "faqs",
                    "JSONValue": "10 faqs array of this form with question and answer, Make the FAQs relevant to Axonator platform, mobile forms, workflows, reports, scheduler, dashboards and integration with third party software"
                },
                {
                    "JSONKey": "form_fields",
                    "JSONValue": "array of 25 fields in the form, group the fields using the Gruop Header field"
                },
                {
                    "JSONKey": "field_type",
                    "JSONValue": "form field type should be one of: [Textbox], [Choice List], [Date Picker], [Time Picker], [Photo Upload], [Documen Upload], [GPS Location], [Audio Upload], [Video Upload], [Textarea], [Number], [Group Header], [Sign]"
                },
                {
                    "JSONKey": "field_description",
                    "JSONValue": "description in the form_fields array"
                },
                {
                    "JSONKey": "placeholder",
                    "JSONValue": "placeholder of the field in the form_fields array"
                },
                {
                    "JSONKey": "options",
                    "JSONValue": "array of strings as options for choice list dropdown selection in the form_fields array"
                },
                {
                    "JSONKey": "validations",
                    "JSONValue": "json of form field value validation rules in the form_fields array, json keys should be in: [required]: true/false, [min_length]: number, [max_length]: number, [min_value]: number, [max_value]: number"
                },
                {
                    "JSONKey": "hint",
                    "JSONValue": "hint of field for user in the form_fields array"
                }
            ],
            "MoreInstructions": [
                "Use only one or two textarea fields, group the fields using the Gruop Header field",
                "JSON top level element should be object",
                "Use exact json keys provided. Make JSON Value length 100 words. All titles must use title case.",
                "Only JSON response. No extra text."
            ]
            }
    ]
}