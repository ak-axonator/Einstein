{
    //What is the name of your checklist?
    "ArtifactName": "",
    "Prompts": [
        //generate checklist prompt
        {
            "Enabled": true,
            "PrefixInstruction": "Give a checklist: ",
            "Artifact": "checklist",
            "GiveContentFor": [
            {
                "JSONKey": "checklist_title",
                "JSONValue": "Checklist Main Title"
            },
            {
                "JSONKey": "checklist_description",
                "JSONValue": "Checklist description in 200 words"
            },
            {
                "JSONKey": "users",
                "JSONValue": "array of 4 users of the checklist in this subcategory"
            },
            {
                "JSONKey": "user_description",
                "JSONValue": "description of the users involved in the app in this subcategory in 30 words"
            },
            {
                "JSONKey": "use",
                "JSONValue": "json of how to use this checklist with and without Axonator"
            },
            {
                "JSONKey": "use_with_axonator",
                "JSONValue": "description in 10 to 15 words on how to use this checklist with Axonator"
            },
            {
                "JSONKey": "use_without_axonator",
                "JSONValue": "description in 10 to 15 words on how to use this checklist without Axonator"
            },
            {
                "JSONKey": "faqs",
                "JSONValue": "give 10 faqs array of this checklist with question and answer, Make the FAQs relevant to Axonator platform, mobile forms, workflows, reports, scheduler, dashboards and integration with third party software"
            },
            {
                "JSONKey": "checkpoints",
                "JSONValue": "give 25 checkpoints list array"
            },
            {
                "JSONKey": "checkpoint",
                "JSONValue": "checkpoint in the checkpoints array"
            },
            {
                "JSONKey": "options",
                "JSONValue": "array of strings as options for checkpoint in the checkpoints array"
            },
            {
                "JSONKey": "required",
                "JSONValue": "true/false for each checkpoint in the checkpoints array"
            },
            {
                "JSONKey": "hint",
                "JSONValue": "hint of checkpoint for user in the checkpoints array"
            }
            ],
            "MoreInstructions": [
                "JSON top level element should be object",
                "Use exact json keys provided. Make JSON Value length 100 words. All titles must use title case.",
                "Give 25 checkpoints",
                "Only JSON response. No extra text."
            ]
        }
    ]
}